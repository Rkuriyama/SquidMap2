			canvas {
				border:1px solid black;

				cursor:crosshair;
				position:absolute;
                box-sizing:border-box;
			}

			canvas#draw_area {
				background-image: url('img/map/hakohugu.png');
				background-size: contain;
				background-repeat: no-repeat;
				background-position: center;
			}

            #tabs-2 {
                padding-left: 5px;
                    padding-right: 0;
            }

            #sidebar {
                width: 200px;
                height: 100%;
                box-sizing:border-box;
                position: absolute;
                left:0;
                top: 0;
            }
                #weapon_list{
                    padding-left: 0;
                    list-style: none;
                    float: left;
                    height:calc( 100% - 80px );
                    overflow: scroll;
                }

                #weapon_list li{
                    display: block;
                    float: left;
                    margin-left:5px;
                    margin-bottom: 5px; 
                }

               #weapon_list li img{
                        border-radius: 5px;
                        border:1px gray solid;
                    }

                .color {
                display: inline-block;
                width: 20px;
                height: 20px;
                vertical-align: middle;
                margin: 5px 5px;
                cursor: pointer;
                border-radius: 5px;
                border:1px solid gray;
                box-shadow: inset 0 0 2px rgba(0,0,0.5);
                }

                    .color.on {
